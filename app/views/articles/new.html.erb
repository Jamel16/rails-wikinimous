<h1>New article</h1>

  <div class="d-flex justify-content-center">
<%= form_with model: @article do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.label :content %>
  <%= f.text_area :content %>
  <%= f.submit %>
<% end %>
  </div>

<%= link_to "Back to list", articles_path, id: "lien" %>
